{
  "name": "@qr-image/lib",
  "version": "0.0.1",
  "description": "QR Code generator (png, svg, pdf, eps)",
  "homepage": "https://github.com/Mr-nUUb/qr-image",
  "keywords": [
    "qrcode",
    "qr code",
    "qr",
    "png",
    "svg",
    "eps",
    "pdf",
    "image"
  ],
  "author": {
    "name": "Mr_nUUb",
    "url": "https://github.com/Mr-nUUb/"
  },
  "license": "MIT",
  "packageManager": "yarn@3.2.0-rc.8",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/Mr-nUUb/qr-image.git"
  },
  "bugs": "https://github.com/Mr-nUUb/qr-image/issues",
  "type": "commonjs",
  "main": "./src/main.ts",
