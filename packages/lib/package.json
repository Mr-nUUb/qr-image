{
  "name": "@qr-image/lib",
  "version": "0.0.12",
  "description": "QR Code generator (png, svg, pdf, eps)",
  "homepage": "https://github.com/Mr-nUUb/qr-image",
  "keywords": [
    "qrcode",
    "qr code",
    "qr",
    "png",
    "svg",
    "eps",
    "pdf",
    "image"
  ],
  "author": {
    "name": "Mr_nUUb",
    "url": "https://github.com/Mr-nUUb/"
  },
  "license": "MIT",
  "packageManager": "yarn@3.2.0-rc.8",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/Mr-nUUb/qr-image.git"
  },
  "bugs": "https://github.com/Mr-nUUb/qr-image/issues",
  "main": "./src/main.ts",
  "publishConfig": {
    "types": "./types/main.d.ts",
    "main": "./commonjs/main.js",
    "browser": "./browser/main.js",
    "module": "./module/main.js",
    "exports": {
      ".": {
        "types": "./types/main.d.ts",
        "main": "./commonjs/main.js",
        "browser": "./browser/main.js",
        "module": "./module/main.js"
      },
      "./json": {
        "types": "./types/base.d.ts",
        "main": "./commonjs/base.js",
        "browser": "./browser/base.js",
        "module": "./module/base.js"
      },
      "./png": {
        "types": "./types/png.d.ts",
        "main": "./commonjs/png.js",
        "browser": "./browser/png.js",
        "module": "./module/png.js"
      },
      "./svg": {
        "types": "./types/svg.d.ts",
        "main": "./commonjs/svg.js",
        "browser": "./browser/svg.js",
        "module": "./module/svg.js"
      },
      "./eps": {
        "types": "./types/eps.d.ts",
        "main": "./commonjs/eps.js",
        "browser": "./browser/eps.js",
        "module": "./module/eps.js"
      },
      "./pdf": {
        "types": "./types/pdf.d.ts",
        "main": "./commonjs/pdf.js",
        "browser": "./browser/pdf.js",
        "module": "./module/pdf.js"
      }
    }
  },
  "files": [
    "/commonjs/**/*.{js,map}",
    "/module/**/*.{js,map}",
    "/browser/**/*.{js,map}",
    "/types/**/*.d.ts",
    "LICENSE"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^14",
    "@types/pako": "^1.0.2"
  },
  "dependencies": {
    "pako": "^2.0.4"
  }
}
