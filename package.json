{
  "name": "@mr-nuub/qr-image",
  "version": "3.2.0",
  "description": "QR Code generator (png, svg, pdf, eps)",
  "homepage": "https://github.com/Mr-nUUb/qr-image",
  "keywords": [
    "qrcode",
    "qr code",
    "qr",
    "png",
    "svg",
    "eps",
    "pdf",
    "image"
  ],
  "author": {
    "name": "Mr_nUUb",
    "url": "https://github.com/Mr-nUUb/"
  },
  "license": "MIT",
  "packageManager": "yarn@3.2.0-rc.7",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/Mr-nUUb/qr-image.git"
  },
  "bugs": "https://github.com/Mr-nUUb/qr-image/issues",
  "type": "commonjs",
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": "./dist/main.js",
    "./base": "./dist/base.js",
    "./eps": "./dist/eps.js",
    "./pdf": "./dist/pdf.js",
    "./png": "./dist/png.js",
    "./svg": "./dist/svg.js",
    "./react": "./dist/react.js"
  },
  "files": [
    "/dist/**/*",
    "LICENSE"
  ],
  "scripts": {
    "build": "run clean && run compile",
    "clean": "rimraf ./dist ./tsconfig.tsbuildinfo",
    "compile": "tsc -b tsconfig.json --verbose",
    "prepack": "run build",
    "test": "jest --runInBand",
    "example:formats:js": "node examples/formats/formats.js",
    "example:formats:ts": "ts-node examples/formats/formats.ts",
    "example:react:js": "webpack serve -c examples/react/webpack.config.js",
    "example:react:ts": "EXAMPLE=tsx webpack serve -c examples/react/webpack.config.js"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@testing-library/react": "^12.1.2",
    "@testing-library/vue": "^6.5.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^14",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/compiler-dom": "^3.2.24",
    "@vue/compiler-sfc": "^3.2.24",
    "babel-loader": "^8.2.3",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.4",
    "jest-canvas-mock": "^2.3.1",
    "path2d-polyfill": "^1.2.0",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "vue": "^3.2.24",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "vue": "^3.0.0"
  }
}
