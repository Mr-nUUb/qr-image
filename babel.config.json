{
  "env": {
    "development": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "12"
            }
          }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": []
    },
    "browser": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": "defaults",
            "modules": false,
            "bugfixes": true
          }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-proposal-optional-chaining"]
    },
    "module": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "14"
            },
            "modules": false
          }
        ],
        "@babel/preset-react",
        "@babel/preset-typescript"
      ],
      "plugins": []
    }
  },
  "comments": false,
  "sourceMaps": "both",
  "minified": true
}
